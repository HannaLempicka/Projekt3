TrackForm:

<TrackForm>:
    orientation: "vertical"
	my_map: map
	txt1: textIn1
	txt2: textIn2
	txt3: textIn3
	txt4: textIn4
	txt5: textIn5
	txt6: textIn6
	txt7: textIn7
	txt8: textIn8
	
    BoxLayout:
        height: "40dp"
        size_hint_y: None
		Button:
            size_hint_x: 25
            text: "Open file"
            on_press: root.open_file()	
        Button:
            size_hint_x: 25
            text: "Analyse file"
            on_press: root.analyse_file()

	
	BoxLayout:
		height: '40dp'
		size_hint_y: None
		Label:
            text: 'horizontal distance'
		Label:
            text: 'elevation gain up'
		Label:
            text: 'elevation gain down'
		Label:
            text: 'entire elevation gain'
			
	BoxLayout:
		height: '30dp'
		size_hint_y: None
		TextInput:
			id: textIn1
			size_hint_x: 25
			font_size: 14
			disabled: 'not'
		TextInput:
			id: textIn2
			size_hint_x: 25
			font_size: 14
			disabled: 'not'
		TextInput:
			id: textIn3
			size_hint_x: 25
			font_size: 14
			disabled: 'not'
		TextInput:
			id: textIn4
			size_hint_x: 25
			font_size: 14
			disabled: 'not'
	
	BoxLayout:
		height: '40dp'
		size_hint_y: None
		Label:
            text: 'Max Height'
		Label:
            text: 'Min Height'
		Label:
            text: 'Mean Speed'
		Label:
            text: 'Practice Time'
			
	BoxLayout:
		height: '30dp'
		size_hint_y: None
		TextInput:
			id: textIn5
			size_hint_x: 25
			font_size: 14
			disabled: 'not'
		TextInput:
			id: textIn6
			size_hint_x: 25
			font_size: 14
			disabled: 'not'
		TextInput:
			id: textIn7
			size_hint_x: 25
			font_size: 14
			disabled: 'not'
		TextInput:
			id: textIn8
			size_hint_x: 25
			font_size: 14
			disabled: 'not'

			
    BoxLayout:
        orientation: 'vertical'
        MapView:
            lat: 0
            lon: 0
            zoom: 1
            id: map
			

<LoadDialog>:
			
	BoxLayout:
		orientation: "vertical"
		FileChooserIconView:
			id: filechooser
			path: './'

		BoxLayout:
			size_hint_y: None
			height: 30
			Button:
				text: "Cancel"
				on_release: root.cancel()
			Button:
				text: "Load"
				on_release: root.load(filechooser.path, filechooser.selection)

	
	